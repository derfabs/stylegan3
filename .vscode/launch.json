{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "generate images",
            "type": "python",
            "request": "launch",
            "program": "gen_images.py",
            "args": [
                "--outdir=out/",
                "--trunc=1",
                "--seeds=0",
                // "--ws=out/zeno_projected_w.npz,out/resa2_projected_w.npz",
                "--network=models/stylegan3-r-ffhq-1024x1024.pkl"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "align",
            "type": "python",
            "request": "launch",
            "program": "alignment.py",
            "args": [
                "--predictor=models/shape_predictor_68_face_landmarks.dat",
                "--source=in/unaligned/",
                "--dest=in/aligned/",
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "create projection dataset",
            "type": "python",
            "request": "launch",
            "program": "dataset_tool.py",
            "args": [
                "--source=in/aligned/",
                "--dest=in/dataset/",
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "project",
            "type": "python",
            "request": "launch",
            "program": "projector.py",
            "args": [
                "--target=in/dataset/00000/img00000011.png",
                "--outdir=out/",
                "--name=resa-met",
                "--num-steps=1000",
                "--save-video",
                "--fps=30",
                "--network=models/stylegan3-r-ffhq-1024x1024.pkl",
                "--feature-ext=models/vgg16.pt"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "interpolate",
            "type": "python",
            "request": "launch",
            "program": "gen_interp_video.py",
            "args": [
                "--network=models/stylegan3-r-ffhq-1024x1024.pkl",
                "--ws=out/zeno_projected_w.npz,out/nicki_projected_w.npz,out/resa_projected_w.npz",
                // "--seeds=1,2,3",
                "--outdir=out/",
                "--name=test",
                "--length=4",
                "--fps=30",
            ],
            "console": "integratedTerminal"
        },
    ]
}