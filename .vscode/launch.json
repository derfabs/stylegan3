{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "generate images",
            "type": "python",
            "request": "launch",
            "program": "gen_images.py",
            "args": [
                "--outdir=out/",
                "--trunc=1",
                "--seeds=0",
                // "--ws=out/zeno_projected_w.npz,out/resa2_projected_w.npz",
                "--network=models/stylegan3-r-ffhq-1024x1024.pkl"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "align",
            "type": "python",
            "request": "launch",
            "program": "alignment.py",
            "args": [
                "--predictor=models/shape_predictor_68_face_landmarks.dat",
                "--source=in/unaligned/",
                "--dest=in/aligned/",
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "create projection dataset",
            "type": "python",
            "request": "launch",
            "program": "dataset_tool.py",
            "args": [
                "--source=in/aligned/",
                "--dest=in/dataset/",
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "project",
            "type": "python",
            "request": "launch",
            "program": "projector.py",
            "args": [
                "--target=in/dataset/00000/img00000000.png",
                "--outdir=out/",
                "--name=zeno-test",
                "--num-steps=1000",
                "--save-video",
                "--fps=30",
                "--network=models/stylegan3-r-ffhq-1024x1024.pkl",
                "--feature-ext=models/vgg16.pt"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "interpolate",
            "type": "python",
            "request": "launch",
            "program": "gen_interp_video.py",
            "args": [
                "--network=models/stylegan3-r-ffhq-1024x1024.pkl",
                "--ws=out/zeno_projected_w.npz,out/nicki_projected_w.npz,out/resa_projected_w.npz",
                // "--seeds=1,2,3",
                "--outdir=out/",
                "--name=test",
                "--length=4",
                "--fps=30",
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "create training dataset",
            "type": "python",
            "request": "launch",
            "program": "dataset_tool.py",
            "args": [
                "--source=in/aligned/",
                "--dest=in/datasets/small256.zip",
                // "--resolution=256x256"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "transfer learn ffhqu-1024",
            "type": "python",
            "request": "launch",
            "program": "train.py",
            "args": [
                "--outdir=training-runs/",
                "--cfg=stylegan3-r",
                "--gpus=1",
                "--batch=32",
                "--gamma=32",
                "--batch-gpu=1",
                "--mbstd-group=1",
                "--snap=10",
                "--mirror=1",
                "--tick=1",
                "--data=in/datasets/small.zip",
                "--kimg=5000",
                "--resume=models/stylegan3-r-ffhqu-1024x1024.pkl"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "transfer learn ffhqu-256",
            "type": "python",
            "request": "launch",
            "program": "train.py",
            "args": [
                "--outdir=training-runs/",
                "--cfg=stylegan3-r",
                "--gpus=1",
                "--batch=32",
                "--gamma=2",
                "--batch-gpu=8",
                "--cbase=16384",
                "--metrics=none",
                "--snap=10",
                "--mirror=1",
                "--tick=1",
                "--data=in/datasets/small256.zip",
                "--kimg=5000",
                "--resume=models/stylegan3-r-ffhqu-256x256.pkl"
            ],
            "console": "integratedTerminal"
        },
    ]
}